{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "regex": "/(\\d{4})/(\\d{2})/(\\d{2})/(.*)([^html])",
        "destination": "/:1/:2/:3/:4:5.html",
        "type": 301
      },
      {
        "source": "/about",
        "destination": "/about.html",
        "type": 301
      },
      {
        "source": "/allPosts",
        "destination": "/allPosts.html",
        "type": 301
      }
    ]
  }
}
